Gramática

    0 $accept: unidad_de_traduccion $end

    1 unidad_de_traduccion: declaracion_externa
    2                     | unidad_de_traduccion declaracion_externa

    3 declaracion_externa: definicion_de_funcion
    4                    | declaracion

    5 definicion_de_funcion: declarador sentencia_compuesta
    6                      | declarador lista_de_declaraciones sentencia_compuesta
    7                      | especificadores_de_declaracion declarador sentencia_compuesta
    8                      | especificadores_de_declaracion declarador lista_de_declaraciones sentencia_compuesta

    9 declaracion: especificadores_de_declaracion ';'
   10            | especificadores_de_declaracion lista_de_declaradores_inicial ';'

   11 lista_de_declaraciones: declaracion
   12                       | lista_de_declaraciones declaracion

   13 especificadores_de_declaracion: especificador_de_clase_de_almacenamiento
   14                               | especificador_de_clase_de_almacenamiento especificadores_de_declaracion
   15                               | especificador_de_tipo
   16                               | especificador_de_tipo especificadores_de_declaracion
   17                               | calificador_de_tipo
   18                               | calificador_de_tipo especificadores_de_declaracion

   19 especificador_de_clase_de_almacenamiento: AUTO
   20                                         | REGISTER
   21                                         | STATIC
   22                                         | EXTERN
   23                                         | TYPEDEF

   24 especificador_de_tipo: VOID
   25                      | CHAR
   26                      | SHORT
   27                      | INT
   28                      | LONG
   29                      | FLOAT
   30                      | DOUBLE
   31                      | SIGNED
   32                      | UNSIGNED
   33                      | especificador_de_struct_o_union
   34                      | especificador_enum

   35 calificador_de_tipo: CONST
   36                    | VOLATILE

   37 especificador_de_struct_o_union: struct_o_union '{' lista_de_declaraciones_struct '}'
   38                                | struct_o_union IDENTIFICADOR '{' lista_de_declaraciones_struct '}'
   39                                | struct_o_union IDENTIFICADOR

   40 struct_o_union: STRUCT
   41               | UNION

   42 lista_de_declaraciones_struct: declaracion_struct
   43                              | lista_de_declaraciones_struct declaracion_struct

   44 lista_de_declaradores_inicial: declarador_inicial
   45                              | lista_de_declaradores_inicial ',' declarador_inicial

   46 declarador_inicial: declarador
   47                   | declarador '=' inicializador

   48 declaracion_struct: lista_de_calificadores_y_especificadores lista_de_declaradores_struct ';'

   49 lista_de_calificadores_y_especificadores: especificador_de_tipo
   50                                         | especificador_de_tipo lista_de_calificadores_y_especificadores
   51                                         | calificador_de_tipo
   52                                         | calificador_de_tipo lista_de_calificadores_y_especificadores

   53 lista_de_declaradores_struct: declarador_struct
   54                             | lista_de_declaradores_struct ',' declarador_struct

   55 declarador_struct: declarador
   56                  | ':' expresion_constante
   57                  | declarador ':' expresion_constante

   58 especificador_enum: ENUM '{' lista_de_enumeradores '}'
   59                   | ENUM IDENTIFICADOR '{' lista_de_enumeradores '}'
   60                   | ENUM IDENTIFICADOR

   61 lista_de_enumeradores: enumerador
   62                      | lista_de_enumeradores ',' enumerador

   63 enumerador: IDENTIFICADOR
   64           | IDENTIFICADOR '=' expresion_constante

   65 declarador: declarador_directo
   66           | puntero declarador_directo

   67 declarador_directo: IDENTIFICADOR
   68                   | '(' declarador ')'
   69                   | declarador_directo '[' ']'
   70                   | declarador_directo '[' expresion_constante ']'
   71                   | declarador_directo '(' lista_de_tipos_de_parametros ')'
   72                   | declarador_directo '(' ')'
   73                   | declarador_directo '(' lista_de_identificadores ')'

   74 puntero: '*'
   75        | '*' lista_de_calificadores_de_tipo
   76        | '*' puntero
   77        | '*' lista_de_calificadores_de_tipo puntero

   78 lista_de_calificadores_de_tipo: calificador_de_tipo
   79                               | lista_de_calificadores_de_tipo calificador_de_tipo

   80 lista_de_tipos_de_parametros: lista_de_parametros
   81                             | lista_de_parametros ',' ELIPSIS

   82 lista_de_parametros: declaracion_de_parametro
   83                    | lista_de_parametros ',' declaracion_de_parametro

   84 declaracion_de_parametro: especificadores_de_declaracion declarador
   85                         | especificadores_de_declaracion
   86                         | especificadores_de_declaracion declarador_abstracto

   87 lista_de_identificadores: IDENTIFICADOR
   88                         | lista_de_identificadores ',' IDENTIFICADOR

   89 inicializador: expresion_de_asignacion
   90              | '{' lista_de_inicializadores '}'
   91              | '{' lista_de_inicializadores ',' '}'

   92 lista_de_inicializadores: inicializador
   93                         | lista_de_inicializadores ',' inicializador

   94 nombre_de_tipo: lista_de_calificadores_y_especificadores
   95               | lista_de_calificadores_y_especificadores declarador_abstracto

   96 declarador_abstracto: puntero
   97                     | declarador_abstracto_directo
   98                     | puntero declarador_abstracto_directo

   99 declarador_abstracto_directo: '(' declarador_abstracto ')'
  100                             | '[' expresion_constante ']'
  101                             | declarador_abstracto_directo '[' expresion_constante ']'
  102                             | '(' lista_de_tipos_de_parametros ')'
  103                             | declarador_abstracto_directo '(' lista_de_tipos_de_parametros ')'

  104 sentencia: sentencia_de_etiqueta
  105          | sentencia_de_expresion
  106          | sentencia_compuesta
  107          | sentencia_de_seleccion
  108          | sentencia_de_iteracion
  109          | sentencia_de_salto

  110 sentencia_de_etiqueta: IDENTIFICADOR ':' sentencia
  111                      | CASE expresion ':' sentencia
  112                      | DEFAULT ':' sentencia

  113 sentencia_de_expresion: ';'
  114                       | expresion ';'

  115 sentencia_compuesta: '{' '}'
  116                    | '{' lista_de_sentencias '}'
  117                    | '{' lista_de_declaraciones '}'
  118                    | '{' lista_de_declaraciones lista_de_sentencias '}'

  119 lista_de_sentencias: sentencia
  120                    | lista_de_sentencias sentencia

  121 sentencia_de_seleccion: IF '(' expresion ')' sentencia IF '(' expresion ')' sentencia ELSE sentencia
  122                       | SWITCH '(' expresion ')' sentencia

  123 sentencia_de_iteracion: WHILE '(' expresion ')' sentencia
  124                       | DO sentencia WHILE '(' expresion ')' ';'
  125                       | FOR '(' ';' ';' ')' sentencia
  126                       | FOR '(' expresion ';' ';' ')' sentencia
  127                       | FOR '(' ';' expresion ';' ')' sentencia
  128                       | FOR '(' ';' ';' expresion ')' sentencia
  129                       | FOR '(' expresion ';' expresion ';' ')' sentencia
  130                       | FOR '(' expresion ';' ';' expresion ')' sentencia
  131                       | FOR '(' ';' expresion ';' expresion ')' sentencia
  132                       | FOR '(' expresion ';' expresion ';' expresion ')' sentencia

  133 sentencia_de_salto: GOTO IDENTIFICADOR ';'
  134                   | CONTINUE ';'
  135                   | BREAK ';'
  136                   | RETURN ';'
  137                   | RETURN expresion ';'

  138 expresion: expresion_de_asignacion
  139          | expresion ',' expresion_de_asignacion

  140 expresion_de_asignacion: expresion_condicional
  141                        | expresion_unaria operador_de_asignacion expresion_de_asignacion

  142 operador_de_asignacion: '='
  143                       | OPERADOR_ASIGNACION_MULTIPLICACION
  144                       | OPERADOR_ASIGNACION_DIVISION
  145                       | OPERADOR_ASIGNACION_RESTO
  146                       | OPERADOR_ASIGNACION_SUMA
  147                       | OPERADOR_ASIGNACION_RESTA
  148                       | OPERADOR_ASIGNACION_DESPLAZAMIENTO_DE_BITS_A_IZQUIERDA
  149                       | OPERADOR_ASIGNACION_DESPLAZAMIENTO_DE_BITS_A_DERECHA
  150                       | OPERADOR_ASIGNACION_AND_BIT_A_BIT
  151                       | OPERADOR_ASIGNACION_XOR_BIT_A_BIT
  152                       | OPERADOR_ASIGNACION_OR_BIT_A_BIT

  153 expresion_condicional: expresion_logica_OR
  154                      | expresion_logica_OR '?' expresion ':' expresion_condicional

  155 expresion_constante: expresion_condicional

  156 expresion_logica_OR: expresion_logica_AND
  157                    | expresion_logica_OR OPERADOR_LOGICO_OR expresion_logica_AND

  158 expresion_logica_AND: expresion_OR_inclusiva
  159                     | expresion_logica_AND OPERADOR_LOGICO_AND expresion_OR_inclusiva

  160 expresion_OR_inclusiva: expresion_XOR
  161                       | expresion_OR_inclusiva '|' expresion_XOR

  162 expresion_XOR: expresion_AND
  163              | expresion_XOR '^' expresion_AND

  164 expresion_AND: expresion_de_igualdad
  165              | expresion_AND '&' expresion_de_igualdad

  166 expresion_de_igualdad: expresion_relacional
  167                      | expresion_de_igualdad OPERADOR_DE_IGUALDAD expresion_relacional
  168                      | expresion_de_igualdad OPERADOR_DE_DESIGUALDAD expresion_relacional

  169 expresion_relacional: expresion_de_desplazamiento
  170                     | expresion_relacional '<' expresion_de_desplazamiento
  171                     | expresion_relacional MENOR_O_IGUAL expresion_de_desplazamiento
  172                     | expresion_relacional '>' expresion_de_desplazamiento
  173                     | expresion_relacional MAYOR_O_IGUAL expresion_de_desplazamiento

  174 expresion_de_desplazamiento: expresion_aditiva
  175                            | expresion_de_desplazamiento OPERADOR_DESPLAZAMIENTO_DE_BITS_A_IZQUIERDA expresion_aditiva
  176                            | expresion_de_desplazamiento OPERADOR_DESPLAZAMIENTO_DE_BITS_A_DERECHA expresion_aditiva

  177 expresion_aditiva: expresion_multiplicativa
  178                  | expresion_aditiva '+' expresion_multiplicativa
  179                  | expresion_aditiva '-' expresion_multiplicativa

  180 expresion_multiplicativa: expresion_de_casteo
  181                         | expresion_multiplicativa '*' expresion_de_casteo
  182                         | expresion_multiplicativa '/' expresion_de_casteo
  183                         | expresion_multiplicativa '%' expresion_de_casteo

  184 expresion_de_casteo: expresion_unaria
  185                    | '(' nombre_de_tipo ')' expresion_de_casteo

  186 expresion_unaria: expresion_posfija
  187                 | INCREMENTO expresion_unaria
  188                 | DECREMENTO expresion_unaria
  189                 | operador_unario expresion_de_casteo
  190                 | SIZEOF expresion_unaria
  191                 | SIZEOF '(' nombre_de_tipo ')'

  192 operador_unario: '~'
  193                | '!'
  194                | '+'
  195                | '-'
  196                | '*'
  197                | '&'

  198 expresion_posfija: expresion_primaria
  199                  | expresion_posfija '[' expresion ']'
  200                  | expresion_posfija '(' ')'
  201                  | expresion_posfija '(' lista_de_expresiones_de_argumentos ')'
  202                  | expresion_posfija '.' IDENTIFICADOR
  203                  | expresion_posfija OPERADOR_FLECHA IDENTIFICADOR
  204                  | expresion_posfija INCREMENTO
  205                  | expresion_posfija DECREMENTO

  206 expresion_primaria: IDENTIFICADOR
  207                   | constante
  208                   | CADENA_DE_CARACTERES
  209                   | '(' expresion ')'

  210 lista_de_expresiones_de_argumentos: expresion_de_asignacion
  211                                   | lista_de_expresiones_de_argumentos ',' expresion_de_asignacion

  212 constante: constante_entera
  213          | CONSTANTE_CARACTER
  214          | CONSTANTE_REAL

  215 constante_entera: CONSTANTE_DECIMAL
  216                 | CONSTANTE_OCTAL
  217                 | CONSTANTE_HEXADECIMAL
  218                 | CONSTANTE_DE_ENUMERACION


